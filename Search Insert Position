class Solution {
    public int searchInsert(int[] nums, int target) {
        int count = 0;
        for(int i=0;i<nums.length;i++){
            if(target>=nums[i]){
                count++;
                if(target == nums[i]){
                count = count - 1;
            }
            }
            
        }
        return count;
    }
};
